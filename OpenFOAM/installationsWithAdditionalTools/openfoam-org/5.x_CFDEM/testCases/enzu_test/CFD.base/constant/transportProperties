// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel HerschelBulkley;

HerschelBulkleyCoeffs
{
    nu0      [ 0 2 -1 0 0 0 0 ] 0.854;
    tau0     [ 0 2 -2 0 0 0 0 ] 8.11E-04;
    k        [ 0 2 -1 0 0 0 0 ] 1.414E-03;
    n        [ 0 0  0 0 0 0 0 ] 0.504;
} 

